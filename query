SELECT
    data_id,
    source_file_path,dataset_id, "timestamp"
FROM public.published_rasters
WHERE dataset_id = 'T0S1P0'
  AND "timestamp" >= TIMESTAMPTZ '2025-01-01 00:00:00+05:30'
  AND ST_Intersects(
        bounds,
        ST_MakeEnvelope(
            68.1, 20.1,
            74.5, 24.7,
            4326
        )
      );


